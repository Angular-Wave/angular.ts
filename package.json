{
  "name": "@angular-wave/angular.ts",
  "description": "A modern, optimized and typesafe version of AngularJS",
  "license": "MIT",
  "version": "0.4.3",
  "type": "module",
  "main": "dist/angular-ts.esm.js",
  "browser": "dist/angular-ts.umd.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Angular-Wave/angular.ts.git"
  },
  "keywords": [
    "AngularJS",
    "AngularTS"
  ],
  "engines": {
    "node": ">=18.18.2"
  },
  "scripts": {
    "serve": "npm-run-all --parallel dev express",
    "dev": "vite",
    "express": "node --watch ./utils/express.js",
    "build": "vite build",
    "playwright": "./node_modules/.bin/playwright test",
    "generate-docs": "node_modules/.bin/typedoc"
  },
  "devDependencies": {
    "@playwright/test": "latest",
    "@rollup/plugin-commonjs": "latest",
    "@rollup/plugin-node-resolve": "latest",
    "@rollup/plugin-terser": "latest",
    "@types/jasmine": "latest",
    "@types/node": "latest",
    "docdash": "latest",
    "eslint": "latest",
    "eslint-config-airbnb-base": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-promise": "latest",
    "express": "latest",
    "jasmine-core": "latest",
    "jsdoc": "latest",
    "npm-run-all": "latest",
    "playwright": "latest",
    "prettier": "latest",
    "rollup": "latest",
    "rollup-plugin-version-injector": "latest",
    "sinon": "latest",
    "typedoc": "latest",
    "typescript": "latest",
    "vite": "latest"
  }
}
